<%- include('partials/header');-%>
<h1>Job interview question in javaScript</h1>
<div class="accordion" id="QAaccordion">
  <% qas.forEach((qa)=>{ %>
  <div class="card">
    <div class="card-header" id="heading:<%=qa.id%>">
      <h2 class="mb-0">
        <button class="btn btn-link" type="button" data-toggle="collapse" data-target="#collapse<%=qa.id%>" aria-expanded="true" aria-controls="collapse:<%=qa.id%>">
          <%=qa.q%>
        </button>
      </h2>
    </div>
    <div id="collapse<%=qa.id%>" class="collapse" aria-labelledby="heading:<%=qa.id%>" data-parent="#QAaccordion">
      <div class="card-body">
        <%=qa.a%>
      </div>
    </div>
  </div>
  <%  });%>
</div>
<%- include('partials/footer');-%>
